<div class="content">
	<div class="description">
		<h4>"This is the order item page, which displays all items in a given order."<h4>
	</div>
    <div class="lable">
        <div class="fn">
            <h3>Item ID</h3>
        </div>
        <div class="ln">
            <h3>Order ID</h3>
        </div>
        <div class="prc">
            <h3>Quantity</h3>
        </div>
    </div>
    <div id="items">
        <div class="itemContainer">
            {{#each orderItem}}
                <section class="orderItem">
                    <div class="item-id">
                        {{itemId}}
                    </div>
                    <div class="order-number">
                        {{orderId}}
                    </div>
                    <div class="quantity">
                    <p>{{quantity}}</p>
                    </div>
                </section>
            {{/each}}
        </div>
    </div>
</div>